{
    "description": "The CodeHuiter framework",
    "name": "madeS/codeHuiterFramework",
    "type": "project",
    "homepage": "https://andrei.bogarevich.com",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "suggest": {
        "paragonie/random_compat": "Provides better randomness in PHP 5.x"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "CodeHuiter\\": "system/"
        }
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.5.6"
    }
}
